AAuH;UAAA;AAA3G,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,YAAL,EAAoB,MAAA,CAAA,KAApB,CAAF;AAAA,KAAA,CAAsG;AAA5C,IAAA,IAAI,EAAC,SAAuC;AAA7B,IAAA,IAAI,EAAC;AAAwB,GAAvH,E;qBAAuG;AAAA,aAAI,C,WAAA,CAAJ;AAAA,K;;GAAvG,C,iCAKI,EAJJ,YAAA,CAA0F,GAA1F,EAA0F,IAA1F,EAA0F,CAAvF,YAAA,CAAmF,GAAnF,EAAmF;AAA9E,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,OAAA,CAAQ,IAAR,yBAA8B,MAAA,CAAA,KAAA,CAAM,EAApC,EAAF;AAAA,KAAA,CAAyE;AAA5B,IAAA,IAAI,EAAC;AAAuB,GAAnF,EAA2E,MAA3E,CAAuF,CAA1F,CAII,EAHwC,MAAA,CAAA,KAAA,CAAM,UAAN,KAAgB,C,kBAA5D,YAAA,CAAqG,GAArG,EAAqG;UAAA;AAAjG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,WAAL,EAAmB,MAAA,CAAA,KAAnB,CAAF;AAAA,KAAA;AAA4F,GAArG,E,CAAkE,W,CAAlE,C,iCAGI,E,aAF6E,Q,CAAS,MAAA,CAAA,KAAA,CAAM,U,mBAAhG,YAAA,CAAgJ,GAAhJ,EAAgJ;UAAA;AAA5I,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,OAAA,CAAQ,IAAR,oCAAyC,MAAA,CAAA,KAAA,CAAM,EAA/C,EAAF;AAAA,KAAA;AAAuI,GAAhJ,E,CAA6G,W,CAA7G,C,iCAEI,E,OADW,Q,CAAS,MAAA,CAAA,KAAA,CAAM,U,mBAA9B,YAAA,CAA8E,GAA9E,EAA8E,WAA9E,EAA8E,CAAnC,WAAmC,CAA9E,C,iCACI,CAbN,CAcI,CA5CN,CA6CI,CAvDN,C","sourcesContent":["<template>\n  <div class=\"order-item\">\n    <div class=\"head\">\n      <span>下单时间：{{order.createTime}}</span>\n      <span>订单编号：{{order.id}}</span>\n      <span class=\"down-time\" v-if=\"order.orderState===1\">\n        <i class=\"iconfont icon-down-time\"></i>\n        <b>付款截止：{{timeText}}</b>\n      </span>\n      <a @click=\"$emit('on-delete', order)\" href=\"javascript:;\" class=\"del\" v-if=\"[5,6].includes(order.orderState)\">删除</a>\n    </div>\n    <div class=\"body\">\n      <div class=\"column goods\">\n        <ul>\n          <li v-for=\"goods in order.skus\" :key=\"goods.id\">\n            <RouterLink class=\"image\" :to=\"`/product/${goods.spuId}`\">\n              <img :src=\"goods.image\" alt=\"\" />\n            </RouterLink>\n            <div class=\"info\">\n              <p class=\"name ellipsis-2\">{{goods.name}}</p>\n              <p class=\"attr ellipsis\">{{goods.attrsText}}</p>\n            </div>\n            <div class=\"price\">¥{{goods.realPay}}</div>\n            <div class=\"count\">x{{goods.quantity}}</div>\n          </li>\n        </ul>\n      </div>\n      <div class=\"column state\">\n        <p>{{orderStatus[order.orderState].label}}</p>\n        <!-- 待收货  查看物流 -->\n        <!-- 待评价  评价商品 -->\n        <!-- 已完成  查看评价 -->\n        <p @click=\"$emit('on-logistics', order)\" v-if=\"order.orderState===3\"><a class=\"green\" href=\"javascript:;\">查看物流</a></p>\n        <p v-if=\"order.orderState===4\"><a class=\"green\" href=\"javascript:;\">评价商品</a></p>\n        <p v-if=\"order.orderState===5\"><a class=\"green\" href=\"javascript:;\">查看评价</a></p>\n      </div>\n      <div class=\"column amount\">\n        <p class=\"red\">¥{{order.payMoney}}</p>\n        <p>（含运费：¥{{order.postFee}}）</p>\n        <p>在线支付</p>\n      </div>\n      <div class=\"column action\">\n        <!-- 待支付：立即付款，查看详情，取消订单 -->\n        <!-- 待发货：查看详情，再次购买 -->\n        <!-- 待收货：确认收货，查看详情，再次购买 -->\n        <!-- 待评价：查看详情，再次购买，申请售后 -->\n        <!-- 已完成：查看详情，再次购买，申请售后 -->\n        <!-- 已取消：查看详情 -->\n        <XtxButton @click=\"$router.push(`/member/pay?orderId=${order.id}`)\" v-if=\"order.orderState===1\" type=\"primary\" size=\"small\">立即付款</XtxButton>\n        <XtxButton @click=\"$emit('on-confirm', order)\" v-if=\"order.orderState===3\" type=\"primary\" size=\"small\">确认收货</XtxButton>\n        <p><a  @click=\"$router.push(`/member/order/${order.id}`)\" href=\"javascript:;\">查看详情</a></p>\n        <p @click=\"$emit('on-cancel', order)\" v-if=\"order.orderState===1\"><a href=\"javascript:;\">取消订单</a></p>\n        <p @click=\"$router.push(`/member/checkout?orderId=${order.id}`)\" v-if=\"[2,3,4,5].includes(order.orderState)\"><a href=\"javascript:;\">再次购买</a></p>\n        <p v-if=\"[4,5].includes(order.orderState)\"><a href=\"javascript:;\">申请售后</a></p>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { orderStatus } from '@/api/constants'\nimport { usePayTime } from '@/hooks'\nexport default {\n  name: 'OrderItem',\n  props: {\n    order: {\n      type: Object,\n      default: () => ({})\n    }\n  },\n  emits: ['on-cancel', 'on-delete', 'on-confirm', 'on-logistics'],\n  setup (props) {\n    const { start, timeText } = usePayTime()\n    start(props.order.countdown)\n\n    return { orderStatus, timeText }\n  }\n}\n</script>\n<style scoped lang=\"less\">\n.order-item {\n  margin-bottom: 20px;\n  border: 1px solid #f5f5f5;\n  .head {\n    height: 50px;\n    line-height: 50px;\n    background: #f5f5f5;\n    padding: 0 20px;\n    overflow: hidden;\n    span {\n      margin-right: 20px;\n      &.down-time {\n        margin-right: 0;\n        float: right;\n        i {\n          vertical-align: middle;\n          margin-right: 3px;\n        }\n        b {\n          vertical-align: middle;\n          font-weight: normal;\n        }\n      }\n    }\n    .del {\n      margin-right: 0;\n      float: right;\n      color: #999;\n    }\n  }\n  .body {\n    display: flex;\n    align-items: stretch;\n    .column {\n      border-left: 1px solid #f5f5f5;\n      text-align: center;\n      padding: 20px;\n      > p {\n        padding-top: 10px;\n      }\n      &:first-child {\n        border-left: none;\n      }\n      &.goods {\n        flex: 1;\n        padding: 0;\n        align-self: center;\n        ul {\n          li {\n            border-bottom: 1px solid #f5f5f5;\n            padding: 10px;\n            display: flex;\n            &:last-child {\n              border-bottom: none;\n            }\n            .image {\n              width: 70px;\n              height: 70px;\n              border: 1px solid #f5f5f5;\n            }\n            .info {\n              width: 220px;\n              text-align: left;\n              padding: 0 10px;\n              p {\n                margin-bottom: 5px;\n                &.name {\n                  height: 38px;\n                }\n                &.attr {\n                  color: #999;\n                  font-size: 12px;\n                  span {\n                    margin-right: 5px;\n                  }\n                }\n              }\n            }\n            .price {\n              width: 100px;\n            }\n            .count {\n              width: 80px;\n            }\n          }\n        }\n      }\n      &.state {\n        width: 120px;\n        .green {\n          color: @xtxColor;\n        }\n      }\n      &.amount {\n        width: 200px;\n        .red {\n          color: @priceColor;\n        }\n      }\n      &.action {\n        width: 140px;\n        a {\n          display: block;\n          &:hover {\n            color: @xtxColor;\n          }\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}