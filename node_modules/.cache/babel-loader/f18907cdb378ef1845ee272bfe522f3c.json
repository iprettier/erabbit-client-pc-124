ion: fixed;\n  z-index: 9999;\n  left: 50%;\n  margin-left: -150px;\n  top: 25px;\n  line-height: 50px;\n  padding: 0 25px;\n  border: 1px solid #e4e4e4;\n  background: #f5f5f5;\n  color: #999;\n  border-radius: 4px;\n  i {\n    margin-right: 4px;\n    vertical-align: middle;\n  }\n  .text {\n    vertical-align: middle;\n  }\n}\n</style>\n"],"sourceRoot":""}]}