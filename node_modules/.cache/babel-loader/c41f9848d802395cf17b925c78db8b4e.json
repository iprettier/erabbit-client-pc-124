: #fff;\n  nav {\n    height: 70px;\n    line-height: 70px;\n    display: flex;\n    border-bottom: 1px solid #f5f5f5;\n    a {\n      padding: 0 40px;\n      font-size: 18px;\n      position: relative;\n      > span {\n        color: @priceColor;\n        font-size: 16px;\n        margin-left: 10px;\n      }\n      &:first-child {\n        border-right: 1px solid #f5f5f5;\n      }\n      &.active {\n        &::before {\n          content: \"\";\n          position: absolute;\n          left: 40px;\n          bottom: -1px;\n          width: 72px;\n          height: 2px;\n          background: @xtxColor;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}